{
  "name": "wedding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "supabase status >/dev/null 2>&1 || supabase start",
    "dev": "dotenv -e .env.local -- next dev",
    "dev:next-only": "dotenv -e .env.local -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "dotenv -e .env.test -- cypress open",
    "cypress:run": "dotenv -e .env.test -- cypress run",
    "cypress:headed": "dotenv -e .env.test -- cypress run --headed --browser chrome",
    "cypress:a11y": "dotenv -e .env.test -- cypress run --spec 'cypress/e2e/accessibility.cy.ts'",
    "test:a11y": "npm run predev:test && dotenv -e .env.test -- start-server-and-test 'npm run dev:test' http://localhost:3907 cypress:a11y",
    "predev:test": "supabase status --workdir supabase-test >/dev/null 2>&1 || supabase start --workdir supabase-test",
    "dev:test": "dotenv -e .env.test -- next dev -p 3907",
    "test:e2e": "npm run predev:test && dotenv -e .env.test -- start-server-and-test 'npm run dev:test' http://localhost:3907 cypress:run",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:test:start": "supabase start --workdir supabase-test",
    "supabase:test:stop": "supabase stop --workdir supabase-test",
    "supabase:test:status": "supabase status --workdir supabase-test",
    "supabase:test:reset": "supabase db reset --workdir supabase-test",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@mantine/core": "^7.15.2",
    "@mantine/form": "^8.2.7",
    "@mantine/hooks": "^7.15.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tabler/icons-react": "^3.26.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ics": "^3.8.1",
    "isomorphic-dompurify": "^2.26.0",
    "next": "15.5.9",
    "posthog-js": "^1.310.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "cypress": "^15.8.1",
    "cypress-axe": "^1.7.0",
    "cypress-plugin-tab": "^1.0.5",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  }
}
